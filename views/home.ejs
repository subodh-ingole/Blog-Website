<%- include("partials/header"); -%>



<%if(posties.length === 0){%>
<h1>No Posts Yet</h1>
<%}else{%>
    <%posties.forEach(function(element){%>
        <h1><%=element.title%></h1> 
        <p><%=element.content.substring(0,100)+"..."%> </p>  
        <a href="/posts/<%=element.title%>">Read More</a>
       
    <%});%>
<%}%>



<%- include("partials/footer"); -%>